{
    "name": "octavianparalescu/daily-tasks",
    "description": "Allows the creation and completion of daily tasks",
    "type": "project",
    "authors": [
        {
            "name": "Octavian Paralescu",
            "email": "me@octav.info"
        }
    ],
    "require": {
        "php": ">=7.4",
        "m1/env": "^2.2",
        "ext-json": "*",
        "pda/pheanstalk": "^4.0",
        "symfony/http-foundation": "^5.1",
        "doctrine/dbal": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DailyTasks\\": [
